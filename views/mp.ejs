
<html>
    <head>
        <!-- CSS only -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
        <style>
            input[type=text] {
                width: 150px;
                box-sizing: border-box;
                border: 2px solid #ccc;
                border-radius: 4px;
                font-size: 16px;
                background-color: white;
                background-image: url('searchicon.png');
                background-position: 10px 10px; 
                background-repeat: no-repeat;
                padding: 12px 20px 12px 40px;
                transition: width 0.4s ease-in-out;
            }

            input[type=text]:focus {
                width: 100%;
            }
            .row {
                background-color: rgb(126, 223, 226);
            }
            
        </style>

    </head>

    <body>

        

        <%=name%>
        <div cla 9 ss="row" style="text-align: center;">

            <div class="col-md-3" style=" display: inline-block; width: 50%; font-size: larger;">
                <i><b><h1>SkyLimit</h1></b></i>
            </div>
            <div class="col-md-6" style=" display: inline-block; width: 20%;">
                <script>
                    const querystring = require('querystring');
                </script>
                <a href="http://localhost:8080/rank?name=<%=name%>&username=<%=username%>"><h2>Rank</h2></a>
            </div>
            <div class="col-md-3" style=" display: inline-block; width: 20%;">
                <h2>Questions</h2>
            </div>
        </div>

        <form align="center" action="/questionsubmit" method="get">
            <!--<h3>Enter your name </h3><input type="text" name="name" value="name"><br><br>-->
            <h3>Enter your Question below<br></h3>
            <input type="text" name="que" placeholder="Search.."><br><br>
            <input type="text" name="dsfs" value="<%= name %>" />
            <input type="submit" value="Submit">
        </form>
        <% for(var i = 0; i < userData.length; i++) { %>
                <% if(userData[i].name == name) { %>
                    <%= userData[i].score%>
                    <%= userData[i].name%>
                <%}%>
                
        <%}%>


    </body>
</html>

