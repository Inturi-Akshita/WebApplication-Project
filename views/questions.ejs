<html>
    <head>

        <style>
            table {
                font-family: arial, sans-serif;
                border-collapse: collapse;
                width: 90%;
            }
          
            td, th {
                border: 1px solid #dddddd;
                text-align: center;
                padding: 8px;
            }
          
            tr:nth-child(even) {
                background-color: #dddddd;
            }
            h1 {
                font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
                margin-top: 40px;
                margin-bottom: 40px;
            }
            body {
                background-image: url('/assets/rankbg1.jpg');
                background-size: cover;
                background-repeat: no-repeat;
                background-position: center;
                image-rendering: -webkit-optimize-contrast;
                width: 100%;
            }
            .x {
                background-color: rgb(255, 255, 255);
            }
            .h {
                background-color: rgb(25, 102, 54);
                color: white;
            }
            
        </style>
    </head>
    
    <body>
        <h1 align="center" style="margin: 40px; font-size: 30px; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif"><b>ALL ENTERED QUESTIONS</b></h1>
        <table class="table" align="center">
            <tr class="h">
                <th>QNo.</th>
                <th>Question</th>
                <th>Name</th>
            </tr>
            <% for(var i = 0; i < queData.length; i++) {%>
            <tr class="x">
                <td class="sno"><%=i + 1%></td>
                <td><%=queData[i].que%></td>
                <td><%=queData[i].name%></td>
            </tr>
            <%}%>
        </table>
    </body>
</html>